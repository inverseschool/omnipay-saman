{
    "name": "soheylak/omnipay-saman",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\Saman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Saman\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "soheyla.karimzade",
            "email": "soheyla.karimzade@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "league/omnipay": "^3",
        "omnipay/paypal": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^4.1.2",
        "phpunit/phpunit": "^8.5.14",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
