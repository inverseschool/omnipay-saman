{
    "name": "inverseschool/omnipay-saman",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\Saman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Saman\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "soheyla.karimzade",
            "email": "soheyla.karimzade@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "omnipay/common": "^3",
        "symfony/http-client": "^5",
        "php-http/guzzle7-adapter": "^1"
    },
    "require-dev": {
        "omnipay/tests": "4.1.1",
        "phpstan/phpstan": "^1.8",
        "squizlabs/php_codesniffer": "^3",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
